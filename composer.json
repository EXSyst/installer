{
    "name": "exsyst/installer",
    "type": "composer-plugin",
    "description": "Allows to easily configure libraries.",
    "license": "MIT",
    "authors": [
        {
            "name": "EXSyst"
        }
    ],
    "require": {
        "php": "^5.5.9|^7.0",
        "composer-plugin-api": "^1.0",
        "symfony/finder": "^3.0"
    },
    "require-dev": {
        "composer/composer": "^1.0",
        "phpunit/phpunit": "^5.0",
        "symfony/phpunit-bridge": "^3.0",
        "squizlabs/php_codesniffer": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "EXSyst\\Installer\\": ""
        }
    },
    "extra": {
        "class": "EXSyst\\Installer\\Plugin",
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    }
}
